<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dashboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.rakeshvasal.myapplication.Activity.Dashboard">

    <include
        android:id="@+id/toolbar"
        layout="@layout/header_layout"/>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/image_capture"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Image Capture"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/location"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Location Search"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/zomato"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Zomato"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/fest"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Oddessey"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/phone_info"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Device Info"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/facebook"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Facebook"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/nested_scroll_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nested Scroll View"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/cricketAPI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Cricket API"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/imagescan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Image Study"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/movieDb"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Movie DB"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />
                <TextView
                    android:id="@+id/create_pdf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Create PDF"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/tv_fingerprint"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="FingerPrint"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/tv_twitter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Twitter"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

                <TextView
                    android:id="@+id/tv_test"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Test"
                    android:textSize="@dimen/activity_text_size"
                    style="@style/MyLinearayoutStyle"
                    />

            </LinearLayout>

        </ScrollView>

    </LinearLayout>

</RelativeLayout>
